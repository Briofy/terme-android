<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LyCardView">
        <attr name="header" format="string" localization="suggested" />
        <attr name="body" format="string" localization="suggested" />
        
        <attr name="type" format="enum">
            <enum name="crypto" value="1" />

            <enum name="testimonial" value="2" />
            <enum name="cta_card" value="13" />

            <enum name="ecommerce" value="9" />

            <enum name="pricing" value="3" />
            <enum name="with_list" value="4" />
            <enum name="form_inputs" value="5" />
            <enum name="user_profile" value="6" />

            <enum name="card_link" value="7" />
            <enum name="card_button" value="14" />

            <enum name="horizontal" value="8" />
            <enum name="card_image" value="10" />

            <enum name="type11" value="11" />
            <enum name="nav_tabs" value="12" />
        </attr>

    </declare-styleable>

    <declare-styleable name="LyCrypto">
        <attr name="helperText" format="string" localization="suggested" />
    </declare-styleable>

    <declare-styleable name="LyTestimonial">
        <attr name="authorName" format="string"  localization="suggested" />
        <attr name="authorAvatar" format="reference" />
        <attr name="authorText" format="string"  localization="suggested" />
    </declare-styleable>

    <declare-styleable name="LyCardButton">
        <attr name="actionButtonText" format="string"  localization="suggested" />
        <attr name="actionButtonIconStart" format="reference" />
    </declare-styleable>

    <declare-styleable name="LyCardLink">
        <attr name="externalLinkText" format="string"  localization="suggested" />
    </declare-styleable>

    <declare-styleable name="LyUserProfile">
        <attr name="userName" format="string"  localization="suggested" />
        <attr name="userWork" format="string"  localization="suggested" />
        <attr name="userImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="LyEcommerce">
        <attr name="ecommerceName" format="string"  localization="suggested" />
        <attr name="ecommerceCast" format="float"  localization="suggested" />
        <attr name="ecommerceRating" format="float"  localization="suggested" />
        <attr name="ecommerceImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="LyProgressBar">
<!--        <attr name="ecommerceName" format="string"  localization="suggested" />-->
<!--        <attr name="ecommerceCast" format="float"  localization="suggested" />-->
<!--        <attr name="ecommerceRating" format="float"  localization="suggested" />-->
<!--        <attr name="ecommerceImage" format="reference" />-->
    </declare-styleable>

    <declare-styleable name="LyTag">
        <attr name="tag" format="string"  localization="suggested" />
    </declare-styleable>
</resources>