<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/root"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">

        <net.lynqfy.offical.tag.LyTag
            android:id="@+id/tag3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:tag="Apple Watch Series 7 GPS" />

        <net.lynqfy.offical.tag.LyTag
            android:id="@+id/tag2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:visibility="invisible"
            app:layout_constraintTop_toBottomOf="@id/tag3"
            app:tag="Apple Watch Series 7 GPS, Aluminium Case, Starlight Sport" />

        <net.lynqfy.offical.tag.LyTag
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            app:layout_constraintTop_toBottomOf="@id/tag2"
            app:tag="Apple Watch Series 7 GPS, Aluminium Case, Starlight Sport" />


        <net.lynqfy.offical.navbar.LyNavBar
            android:id="@+id/navbar1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            app:NavBarType="WithCTAButton" />

        <net.lynqfy.offical.navbar.LyNavBar
            android:id="@+id/navbar0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            app:NavBarType="DoubleDropdown" />


        <net.lynqfy.offical.navbar.LyNavBar
            android:id="@+id/navbar2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            app:NavBarType="LogoCTA" />

        <net.lynqfy.offical.navbar.LyNavBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            app:NavBarType="CenteredLinks" />

        <net.lynqfy.offical.navbar.LyNavBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            app:NavBarType="CTADropdown" />

        <net.lynqfy.offical.navbar.LyNavBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            app:NavBarType="LanguageSearch" />
    </LinearLayout>
</androidx.core.widget.NestedScrollView>
